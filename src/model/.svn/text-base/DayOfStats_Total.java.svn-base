package model;

// 모든 가게 일일 판매수익 전체리스트 목록을 보여주기 위한 클래스 
public class DayOfStats_Total {

	// 디비 컬럼명과 똑같이 쓰기
	
	private int st_num; 	//가게번호
	private String st_name; //가게이름
	private int menu_price; //메뉴가격
	private int quantity;	//팔린수량
	
	//detail항목필드
	private String menu_num;
	private String menu_name;
	private int profit;
	
	//월별매출의 detail항목필드
	private String monthly;
	private int totalProfit;
	
	public int getSt_num() {
		return st_num;
	}
	public void setSt_num(int st_num) {
		this.st_num = st_num;
	}
	public String getSt_name() {
		return st_name;
	}
	public void setSt_name(String st_name) {
		this.st_name = st_name;
	}
	public int getMenu_price() {
		return menu_price;
	}
	public void setMenu_price(int menu_price) {
		this.menu_price = menu_price;
	}
	public int getQuantity() {
		return quantity;
	}
	public void setQuantity(int quantity) {
		this.quantity = quantity;
	}
	
	public int getProfit() {
		return profit;
	}
	public void setProfit(int profit) {
		this.profit = profit;
	}
	public String getMenu_num() {
		return menu_num;
	}
	public void setMenu_num(String menu_num) {
		this.menu_num = menu_num;
	}
	public String getMenu_name() {
		return menu_name;
	}
	public void setMenu_name(String menu_name) {
		this.menu_name = menu_name;
	}
	public String getMonthly() {
		return monthly;
	}
	public void setMonthly(String monthly) {
		this.monthly = monthly;
	}
	public int getTotalProfit() {
		return totalProfit;
	}
	public void setTotalProfit(int totalProfit) {
		this.totalProfit = totalProfit;
	}
	
	
	

}
