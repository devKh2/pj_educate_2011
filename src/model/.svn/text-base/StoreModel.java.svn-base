package model;

import org.springframework.web.multipart.MultipartFile;

public class StoreModel {
	private int st_num;
	private String st_name;
	private String st_type;
	private String st_owner;
	private String st_ownerph;
	private String st_ph;
	private String st_joindate;
	private String st_email;
	private String biz_num;
	private String biz01;
	private String biz02;
	private String biz03;
	private String st_post;
	private String st_sido;
	private String st_gugun;
	private String st_dong;
	private String st_detailaddr;
	private String st_addr;
	private String st_optime;
	private String st_picpath;
	private String menu_sect;
	private MultipartFile file;
	private String st_picnum;
	private String owner_id;
	private String restrictDistance;
	private String guest_cnt;
	private String st_ownerid;
	private int menu_seq;
	private String st_food;

	
	public String getSt_food() {
		return st_food;
	}
	public void setSt_food(String st_food) {
		this.st_food = st_food;
	}
	public String getSt_ownerid() {
		return st_ownerid;
	}
	public void setSt_ownerid(String st_ownerid) {
		this.st_ownerid = st_ownerid;
	}
	public int getMenu_seq() {
		return menu_seq;
	}
	public void setMenu_seq(int menu_seq) {
		this.menu_seq = menu_seq;
	}
	
	public String getGuest_cnt() {
		return guest_cnt;
	}
	public void setGuest_cnt(String guest_cnt) {
		this.guest_cnt = guest_cnt;
	}
	public String getRestrictDistance() {
		return restrictDistance;
	}
	public void setRestrictDistance(String restrictDistance) {
		this.restrictDistance = restrictDistance;
	}
	public String getOwner_id() {
		return owner_id;
	}
	public void setOwner_id(String owner_id) {
		this.owner_id = owner_id;
	}
	public String getSt_picnum() {
		return st_picnum;
	}
	public void setSt_picnum(String st_picnum) {
		this.st_picnum = st_picnum;
	}
	public MultipartFile getFile() {
		return file;
	}
	public void setFile(MultipartFile file) {
		this.file = file;
	}
	
	public String getMenu_sect() {
		return menu_sect;
	}
	public void setMenu_sect(String menu_sect) {
		this.menu_sect = menu_sect;
	}
	public String getSt_picpath() {
		return st_picpath;
	}
	public void setSt_picpath(String st_picpath) {
		this.st_picpath = st_picpath;
	}
	public String getSt_optime() {
		return st_optime;
	}
	public void setSt_optime(String st_optime) {
		this.st_optime = st_optime;
	}
	public int getSt_num() {
		return st_num;
	}
	public void setSt_num(int st_num) {
		this.st_num = st_num;
	}
	public String getSt_name() {
		return st_name;
	}
	public void setSt_name(String st_name) {
		this.st_name = st_name;
	}
	public String getSt_type() {
		return st_type;
	}
	public void setSt_type(String st_type) {
		this.st_type = st_type;
	}
	public String getSt_owner() {
		return st_owner;
	}
	public void setSt_owner(String st_owner) {
		this.st_owner = st_owner;
	}
	public String getSt_ownerph() {
		return st_ownerph;
	}
	public void setSt_ownerph(String st_ownerph) {
		this.st_ownerph = st_ownerph;
	}
	public String getSt_ph() {
		return st_ph;
	}
	public void setSt_ph(String st_ph) {
		this.st_ph = st_ph;
	}
	public String getSt_joindate() {
		return st_joindate;
	}
	public void setSt_joindate(String st_joindate) {
		this.st_joindate = st_joindate;
	}
	public String getSt_email() {
		return st_email;
	}
	public void setSt_email(String st_email) {
		this.st_email = st_email;
	}

	public String getBiz_num() {
		return biz01 + biz02 + biz03;
	}
	public void setBiz_num(String biz_num) {
		this.biz_num = biz_num;
	}

	public String getBiz01() {
		if(biz_num == null)
			return biz01;
		return biz_num.substring(0, 3);
	}
	public void setBiz01(String biz01) {
		this.biz01 = biz01;
	}
	public String getBiz02() {
		if(biz_num == null)
			return biz02;
		return biz_num.substring(3, 5);
	}
	public void setBiz02(String biz02) {
		this.biz02 = biz02;
	}
	public String getBiz03() {
		if(biz_num == null)
			return biz03;
		return biz_num.substring(5, biz_num.length());
	}
	public void setBiz03(String biz03) {
		this.biz03 = biz03;
	}
	public String getSt_post() {
		return st_post;
	}
	public void setSt_post(String st_post) {
		this.st_post = st_post;
	}
	public String getSt_detailaddr() {
		return st_detailaddr;
	}
	public void setSt_detailaddr(String st_detailaddr) {
		this.st_detailaddr = st_detailaddr;
	}
	
	public String getSt_addr() {
		return st_addr;
	}
	public void setSt_addr(String st_addr) {
		this.st_addr = st_addr;
	}
	public String getSt_sido() {
		return st_sido;
	}
	public void setSt_sido(String st_sido) {
		this.st_sido = st_sido;
	}
	public String getSt_gugun() {
		return st_gugun;
	}
	public void setSt_gugun(String st_gugun) {
		this.st_gugun = st_gugun;
	}
	public String getSt_dong() {
		return st_dong;
	}
	public void setSt_dong(String st_dong) {
		this.st_dong = st_dong;
	}

}